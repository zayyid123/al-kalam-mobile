ng mampu mengikat (sekuat) ikatan-Nya.","footnotes":null,"updated_at":null,"surah":{"id":89,"arabic":" الفجر","latin":"Al-Fajr","transliteration":"Al-Fajr","translation":"Fajar","num_ayah":30,"page":593,"location":"Makkiyah"}},{"id":6020,"surah_id":89,"ayah":27,"page":594,"quarter_hizb":60,"juz":30,"manzil":7,"arabic":"يٰٓاَيَّتُهَا النَّفْسُ الْمُطْمَىِٕنَّةُۙ","kitabah":"يٰٓاَيَّتُهَا النَّفْسُ الْمُطْمَىِٕنَّةُۙ","latin":"Yā ayyatuhan-nafsul-muṭma'innah(tu).","arabic_words":["يٰٓاَيَّتُهَا","النَّفْسُ","الْمُطْمَىِٕنَّةُۙ"],"translation":"Wahai jiwa yang tenang,","footnotes":null,"updated_at":null,"surah":{"id":89,"arabic":" الفجر","latin":"Al-Fajr","transliteration":"Al-Fajr","translation":"Fajar","num_ayah":30,"page":593,"location":"Makkiyah"}},{"id":6021,"surah_id":89,"ayah":28,"page":594,"quarter_hizb":60,"juz":30,"manzil":7,"arabic":"ارْجِعِيْٓ اِلٰى رَبِّكِ رَاضِيَةً مَّرْضِيَّةً ۚ","kitabah":"ارْجِعِيْٓ اِلٰى رَبِّكِ رَاضِيَةً مَّرْضِيَّةً ۚ","latin":"Irji‘ī ilā rabbiki rāḍiyatam marḍiyyah(tan).","arabic_words":["ارْجِعِيْٓ","اِلٰى","رَبِّكِ","رَاضِيَةً","مَّرْضِيَّةً ۚ"],"translation":"kembalilah kepada Tuhanmu dengan rida dan diridai.","footnotes":null,"updated_at":null,"surah":{"id":89,"arabic":" الفجر","latin":"Al-Fajr","transliteration":"Al-Fajr","translation":"Fajar","num_ayah":30,"page":593,"location":"Makkiyah"}},{"id":6022,"surah_id":89,"ayah":29,"page":594,"quarter_hizb":60,"juz":30,"manzil":7,"arabic":"فَادْخُلِيْ فِيْ عِبٰدِيْۙ","kitabah":"فَادْخُلِيْ فِيْ عِبٰدِيْۙ","latin":"Fadkhuli fī ‘ibādī.","arabic_words":["فَادْخُلِيْ","فِيْ","عِبٰدِيْۙ"],"translation":"Lalu, masuklah ke dalam golongan hamba-hamba-Ku","footnotes":null,"updated_at":null,"surah":{"id":89,"arabic":" الفجر","latin":"Al-Fajr","transliteration":"Al-Fajr","translation":"Fajar","num_ayah":30,"page":593,"location":"Makkiyah"}},{"id":6023,"surah_id":89,"ayah":30,"page":594,"quarter_hizb":60,"juz":30,"manzil":7,"arabic":"وَادْخُلِيْ جَنَّتِيْ ࣖࣖ","kitabah":"وَادْخُلِيْ جَنَّتِيْ ࣖࣖ